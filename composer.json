{
    "name": "irukasano/phpunit-htmlhint",
    "description": "add assertion of phpunit with htmllint",
    "type": "library",
    "keywords": [
        "phpunit",
        "htmllint"
    ],
    "require": {
        "php": ">=5.6.0,<8.0.0",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "PHPUnitHmtlhint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnitHtmlhint\\Test\\" : "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-workflow": "vendor/bin/phpunit --log-junit results/phpunit.xml"
    }
}
